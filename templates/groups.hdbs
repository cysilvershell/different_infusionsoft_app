<table class="table table-condensed table-striped">
  <thead>
    <tr>
      <th>Tag</th>
      <th>Category</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    {{#groups}}
    <tr>
      <td>{{groupName}}</td>
      <td>{{groupCategoryName}}</td>
      <td>{{dateCreated}}</td>
    </tr>
    {{else}}
    <tr><td colspan="3">No tags available</td></tr>
    {{/groups}}
  </tbody>
</table>
