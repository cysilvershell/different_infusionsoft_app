<methodCall>
  <methodName>{{name}}</methodName>
  <params>
    <param>
      <value><string>{{token}}</string></value>
    </param>
    <param>
      <value><string>{{query}}</string></value>
    </param>
    <param>
      <value>
        <array>
          <data>
          {{#each select}}
            <value><string>{{this}}</string></value>
          {{/each}}
          </data>
        </array>
      </value>
    </param>
  </params>
</methodCall>
