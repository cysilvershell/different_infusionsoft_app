<table class="table table-condensed table-striped">
  <thead>
    <tr>
      <th>Date</th>
      <th>Order</th>
      <th>Total</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    {{#orders}}
    <tr>
      <td>{{dateCreated}}</td>
      <td></td>
      <td></td>
      <td><span class="label label-inverse"></span></td>
    </tr>
    {{else}}
    <tr><td colspan="4">No orders found</td></tr>
    {{/orders}}
  </tbody>
</table>
