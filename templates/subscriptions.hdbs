<table class="table table-condensed table-striped">
  <thead>
    <tr>
      <th>Next bill date</th>
      <th>Plan</th>
      <th>Bill amt.</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    {{#each subscriptions}
    <tr>
      <td>{{nextBillingDate}}</td>
      <td>{{plan}}</td>
      <td>{{billingAmount}}</td>
      <td>{{status}}</td>
    </tr>
    {{else}}
    <tr><td colspan="4">No subscriptions found</td></tr>
    {{/each}}
  </tbody>
</table>
