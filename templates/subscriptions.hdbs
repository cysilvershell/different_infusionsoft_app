<table class="table table-condensed table-striped">
  <thead>
    <tr>
      <th>Next bill date</th>
      <th>Plan</th>
      <th>Bill amt.</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    {{#subscriptions}}
    <tr>
      <td>{{nextBillDate}}</td>
      <td>{{productName}}</td>
      <td>{{billingAmt}}</td>
      <td>{{status}}</td>
    </tr>
    {{else}}
    <tr><td colspan="4">No subscriptions found</td></tr>
    {{/subscriptions}}
  </tbody>
</table>
